{% extends 'base.html.twig' %}

{% block title %}Session{% endblock %}

{% block body %}
    <h1>Session</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ session.id }}</td>
            </tr>
            <tr>
                <th>Theme</th>
                <td>{{ session.theme }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ session.duration }}</td>
            </tr>
            <tr>
                <th>BibliographyReferences</th>
                <td>{{ session.bibliographyReferences }}</td>
            </tr>
            <tr>
                <th>Assignment</th>
                <td>{{ session.assignment }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('session_index') }}">back to list</a>

    <a href="{{ path('session_edit', {'id': session.id}) }}">edit</a>

    {{ include('session/_delete_form.html.twig') }}
